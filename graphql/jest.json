{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**.js"
  ],
  "coverageReporters": [
    "lcov",
    "text",
    "text-summary"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "logHeapUsage": true,
  "verbose": false
}
